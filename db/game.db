 if {$argc!=2} {  
    puts stderr "Usage: %s DATABASE SQL-STATEMENT"
   exit 1
  }
 package require game
 game db [lindex $argv 0]
 db eval [lindex $argv 1] x {
    foreach v $x(*) {
     puts "$v = $x($v)"
   }
    puts ""
  }
const sqlite3 = require('sqlite3')
const db = new sqlite3.Database('game.db');
db.close()